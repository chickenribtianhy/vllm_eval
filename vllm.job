#!/bin/bash -l 
#SBATCH -N 1 
#SBATCH -p GPU-shared
#SBATCH -t 3:30:00 
#SBATCH --gpus=v100-16:2
#SBATCH --output=output.txt 
#SBATCH --error=err.txt 
 
set -x 
 
 
conda activate vllm
export HUGGINGFACE_HUB_CACHE=$PROJECT/.cache/huggingface 

export VLLMHOME=$PROJECT/vllm/vllmevaluation
cd $VLLMHOME

# which python
echo '===================START===================' 


OMP_NUM_THREADS=16 python $VLLMHOME/run.py 2>&1 $VLLMHOME/vllm_run.log
 
echo '====================END====================' 
