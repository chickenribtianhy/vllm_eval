#!/bin/bash -l 
#SBATCH -N 1 
#SBATCH -p GPU-shared
#SBATCH -t 3:30:00 
#SBATCH --gpus=v100-16:4
#SBATCH --output=output2.txt 
#SBATCH --error=err2.txt 
 
set -x 
 
 
conda activate vllm
export HUGGINGFACE_HUB_CACHE=$PROJECT/.cache/huggingface 
export VLLM_CACHE_DIR=$PROJECT/.cache/vllm
export VLLMHOME=$PROJECT/vllm/vllmevaluation
cd $VLLMHOME


echo '===================START===================' 
OMP_NUM_THREADS=16 python $VLLMHOME/run.py 2>&1 $VLLMHOME/vllm_run.log
# python $VLLMHOME/run.py > $VLLMHOME/vllm_run.log
echo '====================END====================' 
